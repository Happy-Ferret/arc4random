{
  "name": "arc4random",
  "version": "1.0.1",
  "description": "Native bindings for the arc4random() family of funcitons.",
  "main": "index.js",
  "scripts": {
    "pretest": "jshint *.js test/*.js",
    "test": "mocha"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/tcort/node-arc4random"
  },
  "keywords": [
    "arc4random",
    "arc4random_buf",
    "arc4random_uniform",
    "random",
    "numbers"
  ],
  "author": "Thomas Cort <linuxgeek@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tcort/node-arc4random/issues"
  },
  "homepage": "https://github.com/tcort/node-arc4random",
  "os": [
    "darwin",
    "freebsd",
    "openbsd",
    "netbsd",
    "linux"
  ],
  "engines": {
    "node": "0.10.x"
  },
  "devDependencies": {
    "expect.js": "^0.3.1",
    "jshint": "^2.5.11",
    "mocha": "^2.0.1"
  },
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "forin": true,
    "freeze": true,
    "globalstrict": true,
    "immed": true,
    "indent": 4,
    "loopfunc": true,
    "moz": true,
    "newcap": true,
    "noarg": true,
    "node": true,
    "noempty": true,
    "nonew": true,
    "trailing": true,
    "smarttabs": true,
    "strict": true,
    "validthis": true,
    "globals": {
      "describe": false,
      "it": false,
      "before": false,
      "beforeEach": false,
      "after": false,
      "afterEach": false,
      "arc4random": false,
      "arc4random_buf": false,
      "arc4random_uniform": false
    }
  }
}
